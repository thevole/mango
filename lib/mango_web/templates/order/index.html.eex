<h2 class="page-title">My Orders</h2>

<table class="table">
  <thead>
    <tr>
      <th>Order ID</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <%= for order <- @orders do %>
      <tr class="order-row">
        <td><%= order.id %></td>
        <td><%= order.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>
